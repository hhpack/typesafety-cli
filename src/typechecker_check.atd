type error_detail = {
  descr: string;
  path: string;
  line: int;
  start: int;
  end: int;
  code: int;
} <ocaml field_prefix="source_">

type error = {
  messages <json name="message">: error_detail list;
} <ocaml field_prefix="error_">

type result = {
  passed: bool;
  errors: error list;
  version: string;
}
